<UserControl x:Class="MvvmWithNavigationService.Views.UserControl3"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MvvmWithNavigationService.Views"
             xmlns:localVms="clr-namespace:MvvmWithNavigationService.ViewModels"
             d:DataContext="{d:DesignInstance Type=localVms:UserControl3ViewModel}"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <Label 
            Name="View3" 
            VerticalAlignment="Top" HorizontalAlignment="Center"
            Content="View 3" Height="40" Canvas.Left="10" Canvas.Top="10" 
            />
            <Button Content="One" Command="{Binding NavigateToOne}"  />
            <Button Content="Two" Command="{Binding NavigateToTwo}"  />
        </StackPanel>
    </Grid>
</UserControl>
